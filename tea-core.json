{
  "$schema": "https://www.schemastore.org/package",
  "classFiles": [
    "build/classes/**/*.class"
  ],
  "targetDirectory": "build/web",
  "targetType": "JAVASCRIPT",
  "mainClass": "net.minecraft.client.Minecraft",
  "sourceMapsFileGenerated": false,
  "minifying": true,
  "optimizationLevel": "SIMPLE",  
  "runtime": "RUNTIME",
  "incremental": false,
  "maxTopLevelNames": 10000,  
  "classesToPreserve": [
    "net.minecraft.**",
    "com.google.**",
    "org.apache.**"
  ],
  "properties": {
    "teavm.js.moduleType": "NONE", 
    "teavm.js.generateQueue": true,
    "teavm.js.longjmp": "emulated"  
  },
  "wasm": {
    "enabled": true              
  },
  "plugins": {
    "jso": {
      "enabled": true             
    },
    "threads": {
      "enabled": false           
    },
    "resources": {
      "enabled": true,            
      "includes": ["**/*.png", "**/*.ogg", "**/*.lang"],
      "excludes": ["**/debug/**"]
    },
    "authors": {
      "author_vm": "shonaax, kiwinatra, laxdude, teavm.org",
      "project_author": "shonaax",
      "sdk_version": "18.2",
      "LTS": true,
      "$api": "https://teavm.org/api/@latest_build"
    },
    "tea_offline": {
      "version": "offline_1.71"
    }
  }
}